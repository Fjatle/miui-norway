msgstr "Mer strømsparings tips"

msgctxt "battery_auto_saving_practise_summary"
msgid ""
"1.Limit number of background apps. \n"
"2.Unless under bright light, there is no need for maximum brightness. \n"
"3.Consider activating Flight Mode at night."
msgstr ""
"1. Begrens antall bakgrunnsprogrammer. \n"
"2. Demp lysstyrken, med mindre du oppholder deg under lyse lysforhold. \n"
"3. Vurder aktivering av flymodus om natten."

msgctxt "battery_auto_saving_rule_apn"
msgid "Disable mobile data"
msgstr "Skru av mobil data"

msgctxt "battery_auto_saving_rule_apn_summary"
msgid "Disable mobile networks such as GPRS and 3G."
msgstr "Deaktiver datanettverk som GPRS og 3G."

msgctxt "battery_auto_saving_rule_auto_sync"
msgid "Disable sync"
msgstr "Skru av synkronisering"

msgctxt "battery_auto_saving_rule_auto_sync_summary"
msgid "Disable syncing of contacts, mail, etc."
msgstr "Skru av synkronisering av kontakter, mail, osv."

msgctxt "battery_auto_saving_rule_background_data"
msgid "Disable BG data transfer"
msgstr "Skru av bakgrunnsoverføring"

msgctxt "battery_auto_saving_rule_background_data_summary"
msgid "Reduces power draw and data charges."
msgstr "Reduserer strømbruken og datakostnader."

msgctxt "battery_auto_saving_rule_bluetooth"
msgid "Disable bluetooth"
msgstr "Skru av bluetooth"

msgctxt "battery_auto_saving_rule_bluetooth_summary"
msgid "Can drastically reduce power draw."
msgstr "Kan drastisk redusere strømbruken."

msgctxt "battery_auto_saving_rule_brightness"
msgid "Reduce brightness"
msgstr "Reduser lysstryke"

msgctxt "battery_auto_saving_rule_brightness_summary"
msgid "Screen brightness will be set to minimum. Can drastically decrease power draw."
msgstr "Skjermens lysstyrke vil bli justert ned til et minimum. Kan drastisk redusere strømbruken."

msgctxt "battery_auto_saving_rule_cpu"
msgid "Reduce CPU speed"
msgstr "Reduser CPU hastighet"

msgctxt "battery_auto_saving_rule_cpu_summary"
msgid "May reduce power draw from rogue applications while the screen is locked."
msgstr "Mulighet for å redusere strømbruken til grådige programmer mens skjermen er låst."

msgctxt "battery_auto_saving_rule_gps"
msgid "Disable GPS"
msgstr "Skru av GPS"

msgctxt "battery_auto_saving_rule_gps_summary"
msgid "Disable background usage of GPS."
msgstr "Skru av bakgrunns bruk av GPS."

msgctxt "battery_auto_saving_rule_haptic"
